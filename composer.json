{
    "name": "acpl/flarum-db-dumper",
    "description": "Dump the contents of a Flarum database",
    "type": "flarum-extension",
    "homepage": "https://github.com/android-com-pl/flarum-db-dumper",
    "license": "MIT",
    "authors": [
        {
            "name": "Rafał Całka",
            "homepage": "https://github.com/rafaucau",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.1",
        "flarum/core": "^1.8",
        "spatie/db-dumper": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "ACPL\\FlarumDbDumper\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "DB Dumper",
            "category": "feature",
            "icon": {
                "name": "fas fa-database",
                "backgroundColor": "#0F9D58",
                "color": "#fff"
            }
        },
        "flarum-cli": {
            "modules": {
                "admin": false,
                "forum": false,
                "js": false,
                "jsCommon": false,
                "gitConf": true,
                "githubActions": true,
                "prettier": false,
                "typescript": false,
                "bundlewatch": false,
                "backendTesting": false,
                "editorConfig": true,
                "styleci": true,
                "css": false
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
